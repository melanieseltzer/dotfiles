[github]
  user = melanieseltzer

[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green

[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold

[color "status"]
	added = yellow
	changed = green
	untracked = red

[core]
	excludesfile = ~/.gitignore_global

[alias]
	a = add .
	ca = commit -a
	cam = commit -am
	cm = commit -m
	s = status
	gst = status
	f = fetch
	fom = fetch origin master
	pom = push origin master
	puom = pull origin master
	cob = checkout -b
	co = checkout